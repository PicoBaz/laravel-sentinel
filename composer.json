{
    "name": "picobaz/laravel-sentinel",
    "description": "Advanced monitoring and alerting system for Laravel applications with real-time notifications",
    "version": "1.3.1",
    "keywords": ["laravel", "monitoring", "performance", "alerting", "sentinel", "logging", "security", "ai", "machine-learning", "predictions", "cost-optimization", "infrastructure", "finops"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "PicoBaz",
            "email": "picobaz3@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "PicoBaz\\Sentinel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PicoBaz\\Sentinel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PicoBaz\\Sentinel\\Providers\\SentinelServiceProvider"
            ],
            "aliases": {
                "Sentinel": "PicoBaz\\Sentinel\\Facades\\Sentinel"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
